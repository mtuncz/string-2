public class S_44 {
    public static void main(String[] args) {
        String str = "fast";
        reverseStr(str);
    }

    public static void reverseStr(String str){
        char[] arr = str.toCharArray();
        for(int i = arr.length-1; i>=0; i--){
            System.out.print(arr[i]);
        }
        
    }
}
